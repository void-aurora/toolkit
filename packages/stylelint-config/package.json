{
  "name": "@void-aurora/stylelint-config",
  "description": "This package provides **SCSS** StyleLint configuration for Void Aurora.",
  "license": "MIT",
  "author": "duduluu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/void-aurora/toolkit.git"
  },
  "version": "0.8.1",
  "main": "index.js",
  "files": [
    "rules"
  ],
  "scripts": {
    "gen-scss": "pnpm run ts -- ./scripts/generate-scss-rules.ts",
    "ts": "ts-node -r tsconfig-paths/register -P ../../tsconfig.scripts.json"
  },
  "sideEffects": false,
  "peerDependencies": {
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.16.0"
  },
  "devDependencies": {
    "axios": "^0.19.2"
  },
  "keywords": [
    "styleguide",
    "stylelint",
    "stylelint-config",
    "void-aurora"
  ],
  "publishConfig": {
    "access": "public"
  }
}
